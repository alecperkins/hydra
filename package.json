{
  "name": "hydrator",

  "version": "1.0.0-alpha.1",
  "scripts": {
    "test": "npm run clearbuild && npm run build:source && jest",
    "prepublishX": "npm test && npm run clearbuild && npm run build",
    "postpublishX": "npm run clearbuild",
    "build": "npm run build:source",
    "build:source": "./node_modules/coffee-script/bin/coffee --output ./lib/ --bare --compile ./source/*",
    "clearbuild": "rm -r ./lib/ && mkdir ./lib/"
  },
  "main": "./lib/index.js",
  "bin": {},
  "dependencies": {
    "archiver": "^1.1.0",
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "bcrypt": "^0.8.7",
    "body": "^5.1.0",
    "cli": "^0.11.1",
    "coffee-react-transform": "^5.0.0",
    "coffee-script": "^1.11.0",
    "commander": "^2.9.0",
    "form-data": "^2.1.0",
    "fs-extra": "^0.26.5",
    "html-minifier": "^3.1.0",
    "marked": "^0.3.6",
    "mime": "^1.3.4",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "request": "^2.75.0",
    "sqwish": "^0.2.2",
    "uglify-js": "^2.7.3",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "jest-cli": "^0.2.2"
  },
  "jest": {
    "testFileExtensions": [
      "coffee",
      "cjsx"
    ],
    "scriptPreprocessor": "<rootDir>/__tests__/_preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "homepage": "http://hydrator.alecperkins.net",
  "bugs": "https://github.com/alecperkins/hydrator",
  "repository": {
    "type": "git",
    "url": "git://github.com/alecperkins/hydrator.git"
  },
  "author": "Alec Perkins <mail@alecperkins.me>",
  "license": {
    "type": "Unlicense",
    "url": "https://raw.github.com/alecperkins/hydrator/master/LICENSE"
  },
  "contributors": [
    {
      "name": "Alec Perkins",
      "email": "mail@alecperkins.me"
    }
  ]
}
